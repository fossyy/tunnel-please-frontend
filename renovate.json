{
  "extends": [
    "config:recommended"
  ],
  "prConcurrentLimit": 1,
  "prHourlyLimit": 1,
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "matchPackagePatterns": ["*"],
      "groupName": "all-dependencies",
      "automerge": true,
      "baseBranchPatterns": [
        "staging"
      ]
    }
  ]
}
